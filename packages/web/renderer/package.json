{
  "name": "hydroxide-dom",
  "description": "Client side renderer for Hydroxide Framework",
  "version": "0.1.0",
  "author": "Manan Tank",
  "license": "MIT",
  "type": "module",
  "main": "dist/prod-cjs.js",
  "module": "dist/prod-esm.js",
  "types": "dist/prod-esm.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": {
        "development": {
          "import": "./dist/dev-esm.js",
          "require": "./dist/dev-cjs.js"
        },
        "import": "./dist/prod-esm.js",
        "require": "./dist/prod-cjs.js"
      },
      "node": {
        "browser": {
          "development": {
            "import": "./dist/dev-esm.js",
            "require": "./dist/dev-cjs.js"
          },
          "import": "./dist/prod-esm.js",
          "require": "./dist/prod-cjs.js"
        }
      }
    }
  },
  "scripts": {
    "build": "rollup -c",
    "ts-check": "tsc --noEmit --project tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "hydroxide": "^0.1.0"
  },
  "peerDependencies": {
    "babel-plugin-hydroxide": "^0.1.0",
    "hydroxide-jsx": "^0.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/dom": "^8.11.3",
    "@types/jest": "^27.0.3",
    "babel-jest": "^28.1.1",
    "babel-plugin-hydroxide": "^0.1.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "hydroxide-jsx": "^0.1.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.7.4"
  }
}